{
  "name": "grocery-list",
  "version": "0.0.1",
  "description": "Generates a grocery list from user-input meals",
  "main": "index.js",
  "scripts": {
    "build": "babel index.js -o index.js && babel groceries.js -o groceries.js && babel meal.js -o meal.js && babel test/ -d test",
    "format": "js-beautify -s 2 -r *.js **/*.js",
    "doc": "jsdoc -R ReadMe.md . -d docs/",
    "test": "mocha",
    "go": "node index.js",
    "softclean": "git clean -dxf -e node_modules/ -e .idea/"
  },
  "author": "Andy Gnias",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.20.0",
    "fuzzy-matching": "^0.4.3",
    "title-case": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "chai": "^4.2.0",
    "js-beautify": "^1.13.0",
    "jsdoc": "^3.6.6",
    "mocha": "^8.1.3"
  }
}
